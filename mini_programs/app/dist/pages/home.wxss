.container{background-color:#e5e5e5}.info{background-color:white;height:224rpx;width:100%;box-sizing:border-box;padding:0 32rpx;display:flex;align-items:center;flex-flow:row nowrap;box-shadow:0 0 16rpx #ccc}.info image{width:156rpx;height:156rpx;border-radius:50%}.info .info-nickname{font-size:48rpx;margin-bottom:16rpx}.info .info-nickname text{overflow:hidden;max-width:380rpx;text-overflow:ellipsis;white-space:nowrap}.info .info-nickname image{width:48rpx;height:48rpx;position:relative;top:8rpx;margin-left:16rpx}.info .info-intro{color:#888}.info .info-intro text{overflow:hidden;max-width:380rpx;text-overflow:ellipsis;white-space:nowrap}.main{margin-top:32rpx;height:calc(100% - 256rpx);width:100%;box-sizing:border-box;overflow-x:hidden;overflow-y:auto;position:relative}.main .main-item{background-color:white;padding:32rpx 64rpx;transition:background-color 0.3s}.main .main-item:active{background-color:#f5f5f5}
